{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "Simple CRUD API application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "start": "ts-node --esm ./src/index.ts",
